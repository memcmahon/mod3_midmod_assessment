<h2>Stations Nearby</h2>

<ul class="station-list">
  <% @stations.each do |station| %>
    <ul class "station">
      <li>Name: <%= station.name %></li>
      <li>Address: <%= station.address %></li>
      <li>Fuel Types: <%= station.fuel_types %></li>
      <li>Distance: <%= station.distance %></li>
      <li>Access_Times: <%= station.access_times %></li>
    </ul>
  <% end %>
</ul>
